#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/jest@30.0.5_@types+node@24.3.0_esbuild-register@3.6.0_esbuild@0.25.9_/node_modules/jest/bin/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/jest@30.0.5_@types+node@24.3.0_esbuild-register@3.6.0_esbuild@0.25.9_/node_modules/jest/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/jest@30.0.5_@types+node@24.3.0_esbuild-register@3.6.0_esbuild@0.25.9_/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/jest@30.0.5_@types+node@24.3.0_esbuild-register@3.6.0_esbuild@0.25.9_/node_modules/jest/bin/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/jest@30.0.5_@types+node@24.3.0_esbuild-register@3.6.0_esbuild@0.25.9_/node_modules/jest/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/jest@30.0.5_@types+node@24.3.0_esbuild-register@3.6.0_esbuild@0.25.9_/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
