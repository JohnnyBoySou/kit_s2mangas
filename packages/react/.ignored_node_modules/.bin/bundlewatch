#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules/bundlewatch/lib/bin/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules/bundlewatch/lib/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules/bundlewatch/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules/bundlewatch/lib/bin/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules/bundlewatch/lib/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules/bundlewatch/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/bundlewatch@0.3.3/node_modules:/mnt/c/Users/joaov/Projetos/s2mangas/kit_s2mangas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bundlewatch/lib/bin/index.js" "$@"
else
  exec node  "$basedir/../bundlewatch/lib/bin/index.js" "$@"
fi
